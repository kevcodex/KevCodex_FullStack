#set("content") {
    <main role="main">

        <section class="jumbotron text-center">
            <div class="container">
                <h1 class="jumbotron-heading">My Cool stuff</h1>
                <p class="lead text-muted">blah blah blah blah</p>
                <p>
                    <a href="#" class="btn btn-primary my-2">DO Something</a>
                    <a href="#" class="btn btn-secondary my-2">Do Something</a>
                </p>
            </div>
        </section>

        <div class="album py-5 bg-light">
            <div class="container">
                <div class="row">
                    #if(projects) {
                        #for(project in projects) {
                            <div class="col-md-4">
                                <div class="card text-right mb-4 box-shadow">
                                    <img class="card-img-top"
                                        alt="Project" src="http://siberian-cat.net/wp-content/uploads/2017/09/yess.jpg"
                                        data-holder-rendered="true" style="height: 100%; width: 100%; display: block;">
                                    <div class="card-body">
                                        <p class="card-text">This one lets the img fit to its aspect ratio.</p>
                                        <div class="d-flex justify-content-center align-items-center">
                                            <div class="btn-group">
                                                <button type="button" class="btn btn-sm btn-outline-primary">View</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    } else {
                        <h2>There arenâ€™t any projects data yet!</h2>
                    }
                </div>
            </div>
        </div>
    </main>
}

#embed("base")